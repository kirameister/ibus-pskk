<?xml version=\"1.0\" encoding=\"utf-8\"?>
<schemalist>
  <schema id='org.freedesktop.ibus.engine.pskk' path='/org/freedesktop/ibus/engine/pskk/'>
    <key name='mode' type='s'>
      <choices>
        <choice value='A'/>
        <choice value='あ'/>
      </choices>
      <default>\"A\"</default>
      <summary>Input Mode</summary>
      <description>
        The current input mode.
      </description>
    </key>
    <key name='dictionary' type='s'>
      <default>\"${pkgdatadir}/restrained.dic\"</default>
      <summary>Kanji Dictionary File</summary>
      <description>
        The pathname to the Kanji dictionary file.
      </description>
    </key>
    <key name='user-dictionary' type='s'>
      <default>\"my.dic\"</default>
      <summary>User Kanji Dictionary File</summary>
      <description>
        The filename of the user Kanji dictionary file.
      </description>
    </key>
    <key name='layout' type='s'>
      <default>\"${pkgdatadir}/layouts/roomazi.json\"</default>
      <summary>Keyboard Layout File</summary>
      <description>
        The pathname to the keyboard layout design file.
      </description>
    </key>
  </schema>
</schemalist>
